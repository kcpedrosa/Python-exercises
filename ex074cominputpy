#Maior e menor valores em Tupla
from random import randint
numeros = (int(input('Digite o 1ยบ numero: ')), int(input('Digite o 2ยบ numero: ')),
           int(input('Digite o 3ยบ numero: ')), int(input('Digite o 4ยบ numero: ')))
print(f'Os numeros digitados foram {numeros}')
print(f'O maior numero dentre os digitados foi {max(numeros)}')
print(f'O menor numero dentre os digitados foi {min(numeros)}')